#using hashtable to reduce time cost
#edition 1
class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        hashtable={}
        for i in range(len(nums)):
            hashtable[nums[i]]=i
        for i in range(len(nums)):
            if target-nums[i] in hashtable:
                if hashtable[target-nums[i]]!=i:
                    return [i,hashtable[target-nums[i]]]
